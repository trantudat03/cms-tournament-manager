{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "playerName1": {
      "type": "string"
    },
    "playerName2": {
      "type": "string"
    },
    "winner": {
      "type": "enumeration",
      "enum": [
        "player1",
        "player2"
      ]
    },
    "startTime": {
      "type": "datetime"
    },
    "endTime": {
      "type": "datetime"
    },
    "matchNumber": {
      "type": "integer"
    },
    "score1": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "score2": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "note": {
      "type": "text"
    },
    "nextMatchWinner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::match.match"
    },
    "nextMatchLoser": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::match.match"
    },
    "previousMatch1": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::match.match"
    },
    "previousMatch2": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::match.match"
    },
    "statusMatch": {
      "type": "enumeration",
      "enum": [
        "pending",
        "in_progress",
        "complated",
        "rejected"
      ]
    },
    "round": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::round.round",
      "inversedBy": "matches"
    },
    "tournament": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tournament.tournament",
      "inversedBy": "matches"
    }
  }
}
